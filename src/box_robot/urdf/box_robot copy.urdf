    	<origin xyz="0 0 0" rpy="0 0 0"/>
			<limit lower="-1.0" upper="0.65" effort="10" velocity="3"/>
        <axis xyz="1 0 0 "/>
	</joint>


 
<link name="base_box2">
		<visual>
		    <origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
				<box size="0.15 0.15 1.3" />
			</geometry>
			
		</visual>	
	</link>


<link name="base_box4">
		<visual>
		    <origin xyz="0 0 0" rpy="1.27 0 0"/>
			<geometry>
				<box size="0.15 0.15 0.8" />
			</geometry>
			
		</visual>	
	</link>
 
<joint name="joint5" type="revolute">
    	<parent link="base_box2"/>
    	<child link="base_box4"/>
    	<origin xyz="0 -0.25 0" rpy="0 0 0"/>
	<limit lower="-0.68" upper="0.65" effort="10" velocity="1"/>
        <axis xyz="1 0 0 "/>
	</joint>

<link name="base_cylinder5">
		<visual>
		    <origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
				<cylinder radius="0.075" length="0.1"/>
			</geometry>
			
		</visual>	
</link>
<joint name="joint6" type="fixed">
    	<parent link="base_box4"/>
    	<child link="base_cylinder5"/>
    	<origin xyz="0 -0.44 0.15" rpy="1.10 0 0"/>
		<limit lower="-0.68" upper="0.65" effort="10" velocity="1"/>
        <axis xyz=" 0 1 0"/>
	</joint>

	
<link name="base_box3">
		<visual>
		    <origin xyz="0 0. 0" rpy="0 0 0"/>
			<geometry>
				<box size="0.12 0.2 0.1" />
