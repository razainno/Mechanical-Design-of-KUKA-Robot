	</joint>
<link name="base_cylinder6">
		<visual>
		    <origin xyz="0 -0.60 1.42" rpy="1.27 0 0"/>
			<geometry>
				<cylinder radius="0.055" length="0.1"/>
			</geometry>
			
		</visual>	
</link>



</robot>